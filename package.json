{
  "name": "climb-bond",
  "author": "yamatohagi",
  "version": "0.0.0",
  "description": "Next & TypeScript",
  "private": true,
  "scripts": {
    "prepare": "panda codegen",
    "dev": "next dev -p 8002",
    "start": "next start",
    "test": "jest --watch",
    "test:ci": "jest",
    "build": "prisma generate && prisma db push && next build",
    "build-netlify": "next build && cp -r .next _next && mv _next .next/",
    "export": "yarn build && next export -o _static",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "clear-all": "rm -rf .next _static node_modules",
    "re-start": "rm -rf .next _static node_modules && yarn install && yarn dev",
    "re-build": "rm -rf .next _static node_modules && yarn install && yarn build",
    "migrate": "prisma migrate dev --name init",
    "migrate-reset": "prisma migrate reset --force"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.462.0",
    "@dnd-kit/core": "^6.1.0",
    "@googlemaps/google-maps-services-js": "^3.3.41",
    "@hookform/resolvers": "^3.3.2",
    "@iconify/react": "^4.1.1",
    "@mui/lab": "^5.0.0-alpha.153",
    "@mui/material": "^5.14.18",
    "@mui/system": "^5.14.18",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.6.0",
    "@radix-ui/react-toggle-group": "^1.0.4",
    "@splidejs/react-splide": "^0.7.12",
    "@supabase/supabase-js": "^2.24.0",
    "@trpc/client": "^10.43.6",
    "@trpc/next": "^10.43.6",
    "@trpc/react-query": "^10.43.6",
    "@trpc/server": "^10.43.6",
    "axios": "^1.6.2",
    "compressorjs": "^1.2.1",
    "cuid": "^3.0.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.5",
    "jotai": "^2.5.1",
    "libphonenumber-js": "^1.10.49",
    "lodash.merge": "^4.6.2",
    "next": "^14.0.3",
    "nodemailer": "^6.9.7",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-easy-crop": "^5.0.2",
    "react-gtm-module": "^2.0.11",
    "react-hook-form": "^7.48.2",
    "react-intersection-observer": "^9.5.3",
    "sharp": "^0.32.6",
    "superjson": "^2.2.1",
    "zod": "^3.22.4",
    "zustand": "^4.4.4"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.18.3",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^20.8.10",
    "@types/nodemailer": "^6.4.14",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.2.34",
    "@types/react-gtm-module": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.52.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "prisma": "^5.6.0",
    "prisma-generator-omit-extra-fields": "^0.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "zod-prisma-types": "^3.1.6"
  }
}
